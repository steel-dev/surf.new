{
  "name": "surf-new",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "uv run python -m uvicorn api.index:app --reload",
    "fastapi-dev:win": "uv run python -m uvicorn api.index:app",
    "next-dev": "next dev -p 3001",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "dev:win": "concurrently \"npm run next-dev\" \"npm run fastapi-dev:win\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "lint:py": "uv run python -m ruff check .",
    "lint:py:fix": "uv run python -m ruff check --fix .",
    "format:py": "uv run python -m ruff format .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:file": "jest --"
  },
  "dependencies": {
